{
  "name": "gt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-components": "ng build components --watch",
    "build-components": "ng build components --configuration production",
    "build-app": "ng build app --base-href /gt/ && npm run copy-index",
